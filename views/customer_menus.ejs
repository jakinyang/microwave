<body>
  <!--Header-->
  <%- include('partials/_header_customer') %>
    <!--Listing-->
    <div class="listings">
      <div class="container">
        <!--Food Type-->
      <%- include('partials/_scroll_bar.ejs') %>

          <div class="header">
            <div class="header-title">
              <h2>What are you craving?</h2>
            </div>
          </div>

          <!--Food Pictures-->
          <div class="listings-grid">
            <!--Image 1-->
            <div class="listing-grid-element">
              <div class="image">
                <img
                  src="https://images.unsplash.com/photo-1626645738196-c2a7c87a8f58?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80"
                  alt="Listing Pic">
              </div>
              <div class="container-row">
                <div class="text">
                  <h3>Fried Chicken</h3>
                  <div class="info">
                    <h6><span class = "label label-default">Price</span> $ 19.99</h6>
                  <h6><span class = "label label-default">Stock</span> 3</h6>
                  </div>
                </div>
                <div class="options">
                  <div class="container add-container">
                    <button class="options-btn selected">Add</button>
                  </div>
                  <button type="button" class="btn btn-primary" data-container="body" data-toggle="popover" data-placement="bottom" data-content="Check me out!">
                    Description
                  </button>
                </div>
              </div>
            </div>
            <!--Image 1 End-->

            <!--Image 2-->
            <div class="listing-grid-element">
              <div class="image">
                <img
                  src="https://images.unsplash.com/photo-1626645738196-c2a7c87a8f58?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80"
                  alt="Listing Pic">
              </div>
              <div class="container-row">
                <div class="text">
                  <h3>Fried Chicken</h3>
                  <div class="info">
                    <span>$19.99 | 20-30 MIN</span>
                  </div>
                </div>
                <div class="options">
                  <div class="container add-container">
                    <button class="options-btn selected">Add</button>
                  </div>
                </div>
              </div>
            </div>
            <!--Image 2 End-->
            <!--Image 3-->
            <div class="listing-grid-element">
              <div class="image">
                <img
                  src="https://images.unsplash.com/photo-1626645738196-c2a7c87a8f58?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80"
                  alt="Listing Pic">
              </div>
              <div class="container-row">
                <div class="text">
                  <h3>Fried Chicken</h3>
                  <div class="info">
                    <span>$19.99 | 20-30 MIN</span>
                  </div>
                </div>
                <div class="options">
                  <div class="container add-container">
                    <button class="options-btn selected">Add</button>
                  </div>
                </div>
              </div>
            </div>
            <!--Image 3 End-->
          </div>

          <!--Check out-->
          <!-- <div class="checkout">
            <button class="checkout-btn">
              Checkout
            </button>
          </div> -->
      </div>
    </div>
    </div>

